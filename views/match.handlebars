<header>
    <img src="images/logo.png" class="logo">
    <nav><img src="images/icons/home.png" class="home-button"></nav>
</header>
<main>
  
    <div class="profile-container">
        <section class="person">
            <img src="images/matching/placeholder-person.png" class="placeholder-person">
            <ul>
                <li class="person-name">{{name}}</li>
                <li class="person-age">{{age}}</li>
                <li class="person-about">{{about}}</li>
            </ul>
        </section>
        <section class="shows">
            <h2>Shows you both watch</h2>
            <ul class="show-list">
                {{#each shows}}
                <li>{{> similairShow }}</li>
                {{/each}}

            </ul>
        </section>
    </div>
    <div class="match-buttons">
        <img src="images/icons/x-mark.png" alt="">
        <img src="images/icons/check-mark.png" alt="">
    </div>
</main>